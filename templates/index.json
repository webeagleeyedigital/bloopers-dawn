          "type": "faq",
          "settings": {
            "heading": "Frequently Asked Questions",
            "heading_size": 24,
            "heading_color": "#000000",
            "paragraph": "Have any queries? We’ve got all the answers covered.",
            "paragraph_size": 16,
            "paragraph_color": "#767474",
            "container_width": 1400,
            "tab_bg_color": "#fafafa",
            "tab_text_color": "#000000",
            "tab_border_color": "#fafafa",
